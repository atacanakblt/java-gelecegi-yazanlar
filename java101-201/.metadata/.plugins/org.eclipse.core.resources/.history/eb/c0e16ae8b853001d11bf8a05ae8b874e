package pojoClasses;

public class Square {

	private int x;
	private int y;
	private int length;

	public int getX() {
		return x;
	}

	public void setX(int _x) {
		if(_x>=0) {
			x = _x;
			
		}else {
			System.out.println("x -1'den büyük olmalıdır");
		}
	}

	public int getY() {
		return y;
	}

	public void setY(int _y) {
		if(_y>=0) {
			y=_y;
			
		}else {
			System.out.println("y -1'den büyük olmalıdır");

		}
	}

	public int getLength() {
		return length;
	}

	public void setLength(int length) {
		this.length = length;
	}

}
