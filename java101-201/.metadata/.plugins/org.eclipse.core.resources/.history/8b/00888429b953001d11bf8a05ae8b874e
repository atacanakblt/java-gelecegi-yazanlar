package pojoClasses;

public class Square {

	private int x;
	private int y;
	private int length;

	public int getX() {
		return x;
	}

	public void setX(int _x) {
		if (_x >= 0) {
			x = _x;

		} else {
			System.out.println("x must be greater than -1");
		}
	}

	public int getY() {
		return y;
	}

	public void setY(int _y) {
		if (_y >= 0) {
			y = _y;

		} else {
			System.out.println("y must be greater than -1");

		}
	}

	public int getLength() {
		return length;
	}

	public void setLength(int _length) {
		if (_length >= 0) {
			length = _length;
		} else {
			System.out.println("length must be greater than -1");
		}
	}

}
