package expressions;

public class Main {

	public static void main(String[] args) {

		// Java da anlamlı en küçük kod bloklarıdır.
		// Declaration tanımlanan bir değişkene ilk değerini atamaktır
		
		int i ; // Expression
		int x = 10; // Declaration
		System.out.println("Selam");
		Math.pow(2, 3);
	}

}
