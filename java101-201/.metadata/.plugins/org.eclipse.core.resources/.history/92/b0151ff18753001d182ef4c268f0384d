package methodParameters;

public class Main {

	public static void main(String[] args) {

		Main driver = new Main();
		int x = 20;
		System.out.println(x);
		driver.passByValue(x);
		System.out.println(x);

	}

	public void degistir(int[]) {
		
	}
	
	public void passByValue(int x) {
		System.out.println("PBV " + x);
		x = 10;
		System.out.println("PBV " + x);

	}

}
