package pojoClasses;

public class Square {

	private int x;
	private int y;
	private int length;

	public int getX() {
		return x;
	}

	public void setX(int _x) {
		if(_x>0) {
			x = _x;
			
		}else {
			System.out.println("x 0'dan büyük olmalıdır");
		}
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

	public int getLength() {
		return length;
	}

	public void setLength(int length) {
		this.length = length;
	}

}
