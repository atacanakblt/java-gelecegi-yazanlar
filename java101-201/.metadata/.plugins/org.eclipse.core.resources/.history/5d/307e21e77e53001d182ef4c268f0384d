package staticMethods2;

public class Main {

	public static void merhaba() {
		System.out.println("Merhaba");
	}
	
	public static void main(String[] args) {

		Main.merhaba();
		
		Math.pow(2, 3);
		
		Math.sqrt(9);
		
		int parseInt = Integer.parseInt("5");
	
		calculator.topla(3, 4);
		calculator.kare(2, 6);
	}

}
