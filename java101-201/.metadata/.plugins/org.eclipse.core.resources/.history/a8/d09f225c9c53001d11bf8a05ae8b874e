package memberVariables2;

public class Driver {

	public static void main(String[] args) {

		Player player1 = new Player();
		System.out.println(player1.health);
		for (int i = 0; i < 30; i++) {
			player1.coup();
			System.out.println(player1.health);

		}
		
		player1.life();
		System.out.println(player1.health);
		

	}

}
