package staticNormalMethods;

public class Main {

	private int x = 20;

	public static void main(String[] args) {
		Main driver = new Main();
		driver.normalMethod();

	}

	public void normalMethod() {
		System.out.println(x);
	}
	
	public static void staticMetot() {
		System.out.println(x);
	}

}
