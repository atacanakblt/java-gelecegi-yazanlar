package logicalOperators;

public class Main {

	public static void main(String[] args) {

		boolean b = true;
		
		System.out.println(b);
		
		System.out.println(!b);
		System.out.println(b & true);

		// false & false = false
		// false + true = false
		// true & false = false
		// true & true = true
		
		// false | false = false
		// false | true = true
		// true | false = true
		// true | true = true
		
		System.out.println(b -);

	}

}
