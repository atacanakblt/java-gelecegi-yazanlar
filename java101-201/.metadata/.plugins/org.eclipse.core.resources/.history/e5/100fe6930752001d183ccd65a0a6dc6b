package arithmeticOperatorsLab;

public class Main {

	public static void main(String[] args) {

		int x =10 ;
		float y = 20;
		System.out.println(x+y);
		System.out.println(x-y);
		System.out.println(x*y);
		System.out.println(x/y);
		System.out.println(8%3);
		
		int a = 10;
		//a = a+2;
		a+=2;
		System.out.println(a);




	}

}
